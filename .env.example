# Configurações do Banco de Dados PostgreSQL
# Adaptado para usar o banco 'reguacobranca' existente
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/reguacobranca

# Configurações JWT (IMPORTANTE: Use uma chave secreta forte em produção)
JWT_SECRET_KEY=sua_chave_secreta_muito_segura_aqui

# Configurações da Aplicação
FLASK_ENV=development
FLASK_DEBUG=True
SECRET_KEY=dev-secret-key

# Configurações opcionais
# Se você tiver um usuário específico para o banco:
# DATABASE_URL=postgresql://seu_usuario:sua_senha@localhost:5432/reguacobranca

